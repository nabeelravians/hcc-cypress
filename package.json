{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "combine-reports": "mochawesome-merge  ./cypress/reports/*.json > ./cypress/mochareports/report.json",
    "generate-report": "marge ./cypress/mochareports/*.json --reportDir mochawesome --assertDir mochawesome/assets --reportPageTitle index.html",
    "test": "cypress run",
    "chrome": "./node_modules/.bin/cypress run --spec cypress/integration/hcc/* --browser chrome",
    "firefox": "./node_modules/.bin/cypress run --spec cypress/integration/hcc/* --browser firefox"
  },
  "author": "nabeel",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.5.1",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1"
  },
  "dependencies": {
    "chai": "^4.3.6"
  }
}
